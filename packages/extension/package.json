{
  "name": "app-ml",
  "displayName": "AppML",
  "description": "Extension for app-ml syntax highlighting.",
  "version": "0.0.1",
  "publisher": "kmandrup",
  "engines": {
    "vscode": "^1.5.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "appml",
        "aliases": [
          "aml",
          "appml"
        ],
        "extensions": [
          ".appml"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "appml",
        "scopeName": "source.appml",
        "path": "./syntaxes/appml.tmLanguage.json"
      }
    ]
  }
}
